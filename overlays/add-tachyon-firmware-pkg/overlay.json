{
    "name": "add-tachyon-firmware-pkg",
    "description": "Adds in tachyon firmware package",
    "commands": [
      {
        "type": "chroot-cmd",
        "cmd": "apt update"
      },
      {
        "type": "chroot-cmd",
        "cmd": "apt install tachyon-firmware"
      },
      {
        "type": "chroot-cmd",
        "cmd": "update-initramfs -k all -t -u"
      }
    ]
  }
  